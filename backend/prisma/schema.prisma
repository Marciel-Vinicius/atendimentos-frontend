generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Chamado {
  id                 Int      @id @default(autoincrement())
  cliente            String
  descricao          String
  status             String
  usuarioRelacionado String
  createdAt          DateTime @default(now())
}
